@model IEnumerable<SBiSchoolWeb.Entities.Transaction>

@{
    ViewBag.Title = "Enquiries";
}

<h2 class="page-title"> Enquiries</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr> 
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostDate)
        </th> 
        <th>
            @Html.DisplayNameFor(model => model.Narrative)
        </th> 
        <th>
            @Html.DisplayNameFor(model => model.TransRef)
        </th> 
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr> 
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostDate)
        </td> 
        <td>
            @Html.DisplayFor(modelItem => item.Narrative)
        </td> 
        <td>
            @Html.DisplayFor(modelItem => item.TransRef)
        </td> 
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
            @*@Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>












@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">

        $(document).ready(function () {

            $("#progress").hide();

            $.ajaxSetup({ cache: false });

        });
        </script>
 
}










