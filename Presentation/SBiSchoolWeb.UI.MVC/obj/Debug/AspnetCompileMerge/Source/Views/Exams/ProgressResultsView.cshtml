            @Html.DisplayFor(modelItem => item.Term)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassShortCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeacherId)
        </td>
