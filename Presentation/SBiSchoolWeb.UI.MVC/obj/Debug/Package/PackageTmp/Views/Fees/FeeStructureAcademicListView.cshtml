@model IEnumerable<SBiSchoolWeb.Entities.FeeStructureAcademic>

@{
    ViewBag.Title = "Fee Structure Academics";
}

<h2 class="page-title"> Fee Structure Academics</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
