@model SBiSchoolWeb.Entities.Subject

@{
    ViewBag.Title = "Subject Details";
    var description = Model.Description.ToUpper();
}

<h2 class="page-title">Details for - @description</h2>


<article class="col1">
    <div class="pad">
 

        <div class="editor-label">
            @Html.LabelFor(model => model.ShortCode)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ShortCode)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Description)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Description)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.OutOf)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.OutOf)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.PassMark)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PassMark)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ROrder)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ROrder)
        </div>



    </div>
</article>

<article class="col2">
    <div class="pad">
 

        <div class="editor-label">
            @Html.LabelFor(model => model.Status)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Status)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Remarks)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Remarks)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.NoOfRequiredHours)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NoOfRequiredHours)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Fees)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Fees)
        </div>


    </div>
</article>









@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">

        $(document).ready(function () {

            $("#progress").hide();

            $.ajaxSetup({ cache: false });

        });
    </script>

}










